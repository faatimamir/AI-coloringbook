
{
  "version": 2,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*\\.(tsx|ts))",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(tsx|ts|js|json|png|jpg|svg|css|pdf|zip)).*)",
      "destination": "/index.html"
    }
  ]
}
